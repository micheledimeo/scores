{
  "name": "mxmlscores",
  "version": "0.9.6",
  "description": "Scores for Nextcloud",
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run move-css",
    "move-css": "mkdir -p css && [ -f js/main.css ] && mv js/main.css css/ || true",
    "watch": "vite build --watch",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "stylelint": "stylelint src/**/*.vue src/**/*.scss src/**/*.css",
    "stylelint:fix": "stylelint src/**/*.vue src/**/*.scss src/**/*.css --fix"
  },
  "dependencies": {
    "@nextcloud/auth": "^2.4.0",
    "@nextcloud/axios": "^2.5.1",
    "@nextcloud/dialogs": "^6.0.0",
    "@nextcloud/event-bus": "^3.3.1",
    "@nextcloud/initial-state": "^2.2.0",
    "@nextcloud/l10n": "^3.1.0",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/vue": "^9.0.0",
    "opensheetmusicdisplay": "^1.8.8",
    "osmd-audio-player": "^0.7.0",
    "vue": "^3.4.0",
    "vue-material-design-icons": "^5.3.0"
  },
  "devDependencies": {
    "@nextcloud/browserslist-config": "^3.0.1",
    "@nextcloud/eslint-config": "^8.4.1",
    "@nextcloud/stylelint-config": "^3.0.1",
    "@nextcloud/vite-config": "^1.4.2",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.57.0",
    "path-browserify": "^1.0.1",
    "sass": "^1.93.2",
    "stream-browserify": "^3.0.0",
    "stylelint": "^16.6.0",
    "vite": "^5.2.0",
    "vm-browserify": "^1.1.2"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Michele"
}

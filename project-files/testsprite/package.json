{
  "name": "mxmlscores-testsprite",
  "version": "1.0.0",
  "description": "Playwright tests for Scores app",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:online": "playwright test --config=playwright.config.online.ts",
    "test:online:ui": "playwright test --config=playwright.config.online.ts --ui",
    "test:online:debug": "playwright test --config=playwright.config.online.ts --debug",
    "test:online:headed": "playwright test --config=playwright.config.online.ts --headed",
    "test:online:simple": "playwright test online-simple.spec.ts",
    "test:online:simple:headed": "playwright test online-simple.spec.ts --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:sidebar": "playwright test sidebar-and-mixer",
    "test:basic": "playwright test app.spec"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "dotenv": "^17.2.3"
  }
}
